<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Login">
   <children>
      <Label layoutX="60.0" layoutY="10.0" prefHeight="36.0" prefWidth="133.0" text="DataBases">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TreeView fx:id="tv" layoutX="60.0" layoutY="50.0" prefHeight="450.0" prefWidth="480.0" />
      <ComboBox fx:id="TableMode" layoutX="60.0" layoutY="520.0" onAction="#Selectmode" prefWidth="150.0" promptText="Select Mode" />
      <Button fx:id="extract" layoutX="225.0" layoutY="520.0" mnemonicParsing="false" onAction="#ExtractButton" prefHeight="20.0" prefWidth="150.0" text="Extract" />
      <Button fx:id="writequery" layoutX="390.0" layoutY="520.0" mnemonicParsing="false" onAction="#queryButton" prefHeight="20.0" prefWidth="150.0" text="Write Query" />
   </children>
</AnchorPane>
